{
    "name": "aurelia-vanilla.presentation-webspa",
    "description": "An Aurelia client application.",
    "version": "0.2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/diegohb/samples-aurelia-vanilla_cli"
    },
    "license": "MIT",
    "engines": {
        "node": "^16.0.0",
        "npm": "please-use-yarn",
        "yarn": "^1.22.0"
    },
    "scripts": {
        "build": "dotnet build && yarn au build",
        "start": "yarn au build && dotnet run",
        "test": "yarn au test",
        "dotnet-pack": "set ASPNETCORE_ENVIRONMENT=Production && dotnet publish -c Release -o ..\\..\\build",
        "au-update":
            "yarn upgrade aurelia-binding@latest aurelia-bootstrapper@latest aurelia-dependency-injection@latest aurelia-event-aggregator@latest aurelia-framework@latest aurelia-history@latest aurelia-history-browser@latest aurelia-loader@latest aurelia-loader-default@latest aurelia-logging@latest aurelia-logging-console@latest aurelia-metadata@latest aurelia-pal@latest aurelia-pal-browser@latest aurelia-path@latest aurelia-polyfills@latest aurelia-route-recognizer@latest aurelia-router@latest aurelia-task-queue@latest aurelia-templating@latest aurelia-templating-binding@latest aurelia-templating-resources@latest aurelia-templating-router@latest aurelia-animator-css@latest",
        "devtooling-versions": "node -v && npm -v && yarn -v && au -v"
    },
    "dependencies": {
        "aurelia-animator-css": "^1.0.4",
        "aurelia-binding": "^2.5.4",
        "aurelia-bootstrapper": "^2.3.3",
        "aurelia-dependency-injection": "^1.5.2",
        "aurelia-event-aggregator": "^1.0.3",
        "aurelia-fetch-client": "^1.8.2",
        "aurelia-framework": "^1.3.1",
        "aurelia-history": "^1.2.1",
        "aurelia-history-browser": "^1.4.0",
        "aurelia-loader": "^1.0.2",
        "aurelia-loader-default": "^1.2.1",
        "aurelia-logging": "^1.5.2",
        "aurelia-logging-console": "^1.1.1",
        "aurelia-metadata": "^1.0.7",
        "aurelia-pal": "^1.8.2",
        "aurelia-pal-browser": "^1.8.1",
        "aurelia-path": "^1.1.5",
        "aurelia-polyfills": "^1.3.4",
        "aurelia-route-recognizer": "^1.3.2",
        "aurelia-router": "^1.7.1",
        "aurelia-task-queue": "^1.3.3",
        "aurelia-templating": "^1.10.4",
        "aurelia-templating-binding": "^1.5.3",
        "aurelia-templating-resources": "^1.13.1",
        "aurelia-templating-router": "^1.4.0",
        "bootstrap": "^5.0.0-beta3",
        "moment": "^2.29.1",
        "popper.js": "^1.16.1",
        "promise-polyfill": "^8.1.3",
        "whatwg-fetch": "^3.6.2"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@types/bootstrap": "^4.3.1",
        "@types/jest": "^27.4.1",
        "@types/jquery": "^3.3.33",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "aurelia-cli": "^2.0.3",
        "aurelia-loader-nodejs": "^1.1.0",
        "aurelia-pal-nodejs": "^2.0.0",
        "aurelia-testing": "^1.0.0",
        "autoprefixer": "^10.4.4",
        "connect": "^3.7.0",
        "connect-history-api-fallback": "^1.6.0",
        "connect-injector": "^0.4.4",
        "cssnano": "^5.1.7",
        "debounce": "^1.2.1",
        "del": "^6.0.0",
        "gulp": "^4.0.2",
        "gulp-dart-sass": "^1.0.2",
        "gulp-eslint": "^6.0.0",
        "gulp-htmlmin": "^5.0.1",
        "gulp-if": "^3.0.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-shell": "^0.8.0",
        "gulp-typescript": "^6.0.0-alpha.1",
        "gulp-watch": "^5.0.1",
        "jest": "^27.5.1",
        "jest-cli": "^27.5.1",
        "jest-transform-stub": "^2.0.0",
        "minimatch": "^5.0.1",
        "node-sass-package-importer": "^5.3.2",
        "open": "^8.4.0",
        "postcss": "^8.4.12",
        "postcss-url": "^10.1.3",
        "promise-polyfill": "^8.2.3",
        "requirejs": "^2.3.6",
        "serve-static": "^1.15.0",
        "server-destroy": "^1.0.1",
        "socket.io": "^4.4.1",
        "text": "requirejs/text",
        "ts-jest": "^27.1.4",
        "typescript": "^4.6.3",
        "eslint": "^8.12.0"
    },
    "jest": {
        "moduleNameMapper": {
            "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding",
            "^.+\\.css$": "<rootDir>/test/mock-css"
        },
        "modulePaths": [
            "<rootDir>/src",
            "<rootDir>/node_modules"
        ],
        "moduleFileExtensions": [
            "ts",
            "js",
            "json"
        ],
        "transform": {
            "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
                "jest-transform-stub",
            "^.+\\.ts$": "ts-jest"
        },
        "testRegex": "\\.spec\\.(ts|js)$",
        "setupFiles": [
            "<rootDir>/test/jest-pretest.ts"
        ],
        "testEnvironment": "node",
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,ts}",
            "!**/*.spec.{js,ts}",
            "!**/node_modules/**",
            "!**/test/**"
        ],
        "coverageDirectory": "<rootDir>/test/coverage-jest",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ]
    }
}